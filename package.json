{
  "name": "uask-sys",
  "version": "1.1.5",
  "description": "Data management system for Spiral",
  "main": "./dist/client/index.js",
  "exports": {
    ".": "./dist/client/index.js",
    "./pkce": "./dist/client/pkce.js",
    "./example": "./dist/client/example.js"
  },
  "types": "./dist/client/index.d.ts",
  "typesVersions": {
    "*": {
      "pkce": [
        "dist/client/pkce.d.ts"
      ],
      "example": [
        "dist/client/example.d.ts"
      ]
    }
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "node --loader ts-node/esm ./test-runner.ts",
    "cover": "./node_modules/.bin/c8 --include 'src' npm test",
    "doc": "./node_modules/.bin/typedoc ./src/index.ts",
    "build-client": "./node_modules/.bin/rollup -c rollup.client.js",
    "build-server": "./node_modules/.bin/rollup -c rollup.server.js",
    "build": "./node_modules/.bin/tsc ; npm run build-client ; npm run build-server",
    "lint": "./node_modules/.bin/eslint . --ext .ts",
    "start": "./node_modules/.bin/node -r dotenv/config dist/server",
    "migrate": "npm run knex migrate:latest",
    "seed": "npm run knex seed:run",
    "knex": "NODE_ENV=$APP_ENV node --loader ts-node/esm -r dotenv/config ./node_modules/knex/bin/cli",
    "serve": "nodemon --loader ts-node/esm -r dotenv/config src/server.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/u-ask/uask-sys.git"
  },
  "author": "",
  "license": "MIT",
  "homepage": "https://github.com/u-ask/uask-sys#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/adm-zip": "^0.5.0",
    "@types/archiver": "^5.3.1",
    "@types/benchmark": "^2.1.1",
    "@types/body-parser": "^1.19.2",
    "@types/copy-paste": "^1.1.30",
    "@types/debug": "^4.1.7",
    "@types/deep-equal": "^1.0.1",
    "@types/ejs": "^3.1.1",
    "@types/got": "^9.6.12",
    "@types/knex": "^0.16.1",
    "@types/marked": "^4.0.3",
    "@types/microtime": "^2.1.0",
    "@types/node": "^17.0.35",
    "@types/oidc-provider": "^7.11.1",
    "@types/rewire": "^2.5.28",
    "@types/sanitize-html": "^2.6.2",
    "@types/sinon": "^10.0.11",
    "@types/tape": "^4.13.2",
    "@types/temp": "^0.9.1",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "@wessberg/rollup-plugin-ts": "^2.0.4",
    "adm-zip": "^0.5.9",
    "better-sqlite3": "^7.5.3",
    "c8": "^7.11.3",
    "csv-parse": "^5.1.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-radar": "^0.2.1",
    "html-validate": "^7.1.1",
    "jose2": "npm:jose@^4.8.1",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "rollup": "^2.74.1",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-dts": "^4.2.2",
    "sinon": "^14.0.0",
    "tape": "^5.5.3",
    "temp": "^0.9.4",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.0",
    "tslib": "^2.4.0",
    "typedoc": "^0.22.15",
    "typescript": "^4.7.2",
    "winston": "^3.7.2"
  },
  "dependencies": {
    "@pothos/core": "^3.11.0",
    "@sendgrid/mail": "^7.7.0",
    "@sindresorhus/fnv1a": "^3.0.0",
    "archiver": "^5.3.1",
    "await-semaphore": "^0.1.3",
    "body-parser": "^1.20.0",
    "csv-stringify": "^6.1.0",
    "debug": "^4.3.4",
    "ejs": "^3.1.8",
    "express-graphql": "^0.12.0",
    "fast-deep-equal": "^3.1.3",
    "got": "^12.1.0",
    "graphql": "^16.5.0",
    "helmet": "^5.1.0",
    "jose": "^4.8.1",
    "jwt-decode": "^3.1.2",
    "knex": "^2.1.0",
    "marked": "^4.0.16",
    "oidc-provider": "^7.11.3",
    "oidc-provider-knex-adapter": "^1.3.0",
    "openid-client": "^5.1.6",
    "pg": "^8.7.3",
    "restana": "^4.9.5",
    "sanitize-html": "^2.7.0",
    "serve-static": "^1.15.0",
    "stealer": "^1.0.6",
    "twilio": "^3.77.1",
    "uask-auth": "^1.1.8",
    "uask-dom": "^1.1.1",
    "uuid-random": "^1.3.2"
  },
  "peerDependencies": {
    "spiral-dom": "^1.0.9"
  }
}
